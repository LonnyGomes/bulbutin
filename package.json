{
  "name": "bulbutin",
  "version": "0.0.1",
  "description": "Bulbutin app",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:parse": "cd packages/parse-photos/ && npm run build",
    "build:ng": "cd apps/bulbutin-ng/ && ng build -c production",
    "deploy:ng": "npm run build:ng && rm -rf dist && mkdir -p dist && cp -r apps/bulbutin-ng/dist/bulbutin-ng/browser/* dist/",
    "cp:images": "mkdir -p apps/bulbutin-ng/public/assets/bulbutin-images && cp packages/parse-photos/images/* apps/bulbutin-ng/public/assets/bulbutin-images/",
    "postinstall": "npm run build:parse && npm run cp:images"
  },
  "author": "Lonny Gomes",
  "license": "ISC",
  "workspaces": [
    "packages/bulbutin-vue-app",
    "packages/parse-photos",
    "apps/*"
  ]
}
